{
    "name": "xactsystems/type-hint-hydrator",
    "description": "A Symfony object hydrator based on declared types and annotation type-hints.",
    "keywords": ["object", "hydrator", "array", "typed properties", "annotation", "symfony", "doctrine", "entity", "request"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Foulds",
            "email": "ianfoulds@x-act.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xact\\TypeHintHydrator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xact\\TypeHintHydrator\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^3.2",
        "jms/serializer-bundle": "^5.0",
        "laminas/laminas-hydrator": "^4.15",
        "nette/utils": "4.0",
        "symfony/validator": "^6.2||7.1",
        "symfony/http-foundation": "^6.2||7.1"
    },
    "require-dev": {
        "symfony/debug-pack": "*",
        "symfony/test-pack": "*"
    }
}
